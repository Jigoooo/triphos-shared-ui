{
  "name": "@triphos/shared-ui",
  "version": "0.0.0",
  "type": "module",
  "description": "Modern React component library built with TypeScript and Vite",
  "keywords": [
    "react",
    "components",
    "typescript",
    "ui",
    "library"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "homepage": "https://github.com/your-username/my-react-library#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/my-react-library.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/your-username/my-react-library/issues"
  },
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs",
      "types": "./dist/index.d.ts"
    },
    "./styles": "./dist/style.css",
    "./package.json": "./package.json"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "tsc && vite build",
    "build:watch": "tsc && vite build --watch",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "clean": "rm -rf dist",
    "clean:deps": "rm -rf node_modules",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json}\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepublishOnly": "npm run clean && npm run build && npm run test",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": false
    }
  },
  "dependencies": {
    "@floating-ui/react": "^0.27.12",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.9",
    "@tanstack/table-core": "^8.21.3",
    "@uiw/react-color": "^2.5.5",
    "@uiw/react-color-circle": "^2.5.5",
    "@uiw/react-textarea-code-editor": "^3.1.1",
    "axios": "^1.9.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "emoji-picker-react": "^4.12.2",
    "file-saver": "^2.0.5",
    "framer-motion": "^12.16.0",
    "isomorphic-dompurify": "^2.25.0",
    "keepalive-for-react": "^4.0.2",
    "keepalive-for-react-router": "^2.0.2",
    "lenis": "^1.3.4",
    "lucide-react": "^0.513.0",
    "polished": "^4.3.1",
    "re-resizable": "^6.11.2",
    "react-daum-postcode": "^3.2.0",
    "react-device-detect": "^2.2.3",
    "react-icons": "^5.5.0",
    "react-image-file-resizer": "^0.4.8",
    "react-intersection-observer": "^9.16.0",
    "react-lazy-load-image-component": "^1.6.3",
    "react-medium-image-zoom": "^5.2.14",
    "react-naver-maps": "^0.1.4",
    "react-responsive-carousel": "^3.2.23",
    "react-scripts": "5.0.1",
    "react-spinners": "^0.17.0",
    "react-spring": "^10.0.1",
    "react-swipeable": "^7.0.2",
    "react-syntax-highlighter": "^15.6.1",
    "react-use-gesture": "^9.1.3",
    "react-use-measure": "^2.1.7",
    "rehype-prism-plus": "^2.0.1",
    "rehype-rewrite": "^4.0.2",
    "sonner": "^2.0.5",
    "uuid": "^11.1.0",
    "vite-plugin-svgr": "^4.3.0",
    "xlsx-js-style": "^1.2.0",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@types/file-saver": "^2.0.7",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.29",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@types/react-lazy-load-image-component": "^1.6.4",
    "@types/react-swipeable-views": "^0.13.6",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.5.1",
    "@vitejs/plugin-react-swc": "^3.10.1",
    "babel-plugin-react-compiler": "19.1.0-rc.2",
    "globals": "^16.2.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "sharp": "^0.34.2",
    "svgo": "^3.3.2",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-image-optimizer": "^1.1.8",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
