{
  "name": "@jigoooo/shared-ui",
  "version": "0.0.225",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "description": "A reusable React component library and design system with TypeScript support, built on Vite for seamless integration and optimized performance.",
  "keywords": [
    "react",
    "typescript",
    "component-library",
    "ui-library",
    "design-system",
    "shared-ui",
    "vite",
    "responsive-components",
    "themeable",
    "accessibility",
    "storybook",
    "monorepo",
    "pnpm-workspace",
    "modular-ui",
    "reusable-components",
    "tailwind-compatible",
    "chakra-ui-compatible",
    "material-ui-compatible"
  ],
  "author": "jiwook97@gmail.com",
  "license": "MIT",
  "homepage": "https://jigoooo.github.io/triphos-shared-ui/",
  "repository": {
    "type": "git",
    "url": "https://github.com/Jigoooo/triphos-shared-ui.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/Jigoooo/triphos-shared-ui/issues"
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./dist/shared-ui.css": "./dist/shared-ui.css"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "tsc && vite build",
    "build:watch": "tsc && vite build --watch",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "clean": "rm -rf dist",
    "clean:deps": "rm -rf node_modules",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json}\"",
    "prepublishOnly": "npm run clean && npm run build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "storybook dev -p 4600",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "react": ">=19.1.0",
    "react-dom": ">=19.1.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": false
    }
  },
  "dependencies": {
    "@floating-ui/react": "^0.27.16",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.12",
    "polished": "^4.3.1",
    "react-icons": "^5.5.0",
    "sonner": "^2.0.7",
    "uuid": "^11.1.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@types/file-saver": "^2.0.7",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@types/react-swipeable-views": "^0.13.6",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^5.0.1",
    "@vitejs/plugin-react-swc": "^4.0.1",
    "babel-plugin-react-compiler": "19.1.0-rc.2",
    "globals": "^16.3.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "sharp": "^0.34.3",
    "svgo": "^4.0.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.3",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-image-optimizer": "^2.0.2",
    "vite-plugin-svgr": "^4.3.0",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
